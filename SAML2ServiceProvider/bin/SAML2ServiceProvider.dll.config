<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!--<add key="idpssoURL" value="http://localhost:51388/SAML2IdentityProvider/SAML/SSOService.aspx"/>
    <add key="idpArtifactResponderURL" value="http://localhost:51388/SAML2IdentityProvider/SAML/ArtifactResponder.aspx"/>
    <add key="idpLogoutURL" value="http://localhost:51388/SAML2IdentityProvider/SAML/SLOService.aspx"/>-->

    <add key="idpssoURL" value="http://localhost:51388/SAML/SSOService.aspx"/>
    <add key="idpArtifactResponderURL" value="http://localhost:51388/SAML/ArtifactResponder.aspx"/>
    <add key="idpLogoutURL" value="http://localhost:51388/SAML/SLOService.aspx"/>
  </appSettings>
  <connectionStrings/>
  <system.web>
    <compilation debug="true" targetFramework="4.0"/>
    <authentication mode="Forms">
      <forms name="saml2serviceprovider" loginUrl="loginchoice.aspx">
      </forms>
    </authentication>
    <authorization>
      <deny users="?"/>
    </authorization>
    <customErrors mode="RemoteOnly"/>
    <trace enabled="true" writeToDiagnosticsTrace="true"/>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
  </system.web>
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="CyclicTextWriter" />
      </listeners>
    </trace>
    <sources>
      <source name="ComponentSpace.SAML2" switchValue="Verbose">
        <listeners>
          <add name="CyclicTextWriter" />
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <!-- Ensure IIS has create/write file permissions for the log folder. -->
      <add name="CyclicTextWriter" type="ComponentSpace.SAML2.Utility.CyclicTraceListener,ComponentSpace.SAML2" initializeData="logs" />
    </sharedListeners>
  </system.diagnostics>
</configuration>