<h1>Activants Pte Ltd Service Provider Started...</h1>
@using System.Security.Claims;
@using Microsoft.AspNet.Identity
@if (Session["attr"] != null)
{

<div class="container">
    <div class="jumbotron" style="background-color:yellowgreen">
       
        <div class="row">
            <center>
                <p style="color:#fff">SAML Data of @User.Identity.GetUserName()</p>
                <div class="col-sm-12">
                    <table class="table table-bordered">
                        @foreach (var claim in Session["attr"] as Dictionary<string, string>)
                        {
                            <tr class="success" style="color:black;">
                                <td title="@claim.Key">@claim.Key</td>
                                <td>@claim.Value</td>
                            </tr>

                        }
                    </table>

                </div>
                <center>
                    <p style="color:#fff">OWIN data of @User.Identity.GetUserName()</p>
                    <div class="col-sm-12">
                        <table class="table table-bordered">
                            @foreach (var claim in ((ClaimsIdentity)Context.User.Identity).Claims)
                            {
                                <tr class="success" style="color:black;">
                                    <td title="@claim.Type">@claim.Type</td>
                                    <td>@claim.Value</td>
                                </tr>

                            }
                        </table>

                    </div>
                </center>
            </center>
        </div>
    </div>
</div>
}
